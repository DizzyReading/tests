    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "latest",
    "jest": "^27.2.0",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^7.0.5",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
